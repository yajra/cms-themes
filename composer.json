{
    "name": "yajra/cms-themes",
    "description": "Themes module of YajraCMS.",
    "keywords": [
        "yajracms",
        "laravel",
        "themes"
    ],
    "homepage": "https://github.com/yajra/cms-themes",
    "license": "MIT",
    "authors": [
        {
            "name": "Arjay Angeles",
            "email": "aqangeles@gmail.com",
            "homepage": "http://yajrabox.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "yajra/cms-core": "*"
    },
    "require-dev": {
        "phpunit/phpunit" : "7.*"
    },
    "autoload": {
        "psr-4": {
            "Yajra\\CMS\\Themes\\": "src"
        },
        "files": [
          "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Yajra\\CMS\\Themes\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        },
        "laravel": {
            "providers": [
                "Yajra\\CMS\\Themes\\ThemesServiceProvider"
            ],
            "facades": [
                "Yajra\\CMS\\Themes\\Facades\\Theme"
            ]
        }
    }
}
